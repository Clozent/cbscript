desc "Test for new data storage features"

import common

function recurse(x, y)
	if x >= 0
		recurse(x-1, y+1)
		tell @a '(x) (y)'
	end
end	

function factorial(n)
	if n <= 1
		return 1
	else
		return n * factorial(n-1)
	end
end

reset
	/data modify storage $namespace stack set value []
	recurse(5, 5)
	val = factorial(5)
	tell @a '(val)'
end
