desc "Test the performance of different operations"

import common

reset
	/kill @Entity[type=!player]
	
	for i=1 to 10000
		create @Marker
	end
	
	first = True
	
	tell @a '[\[Run Test\]](call run_test)'
end

function run_test()
	/say Marker[dummy].dummy = 0
	for i = 1 to 1000
		@Marker[dummy].dummy = 0
	end
	/say done.
	
	/say as Marker if s.dummy s.dummy = False
	for i = 1 to 1000
		as @Marker if @s.dummy
			@s.dummy = False
		end
	end
	/say done.
end